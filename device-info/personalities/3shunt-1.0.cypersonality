      <ConfigDefine name="`${INST_NAME}`_PORT" value="`${&quot;GPIO_PRT&quot;. port}`" public="true" include="true" />
      <ConfigDefine name="`${INST_NAME}`_PORT_NUM" value="`${port}`U" public="true" include="true" />
